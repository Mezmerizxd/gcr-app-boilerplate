{
  "name": "gcr-app-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "lerna run start --stream --parallel",
    "watch": "lerna run watch --stream --parallel",
    "build": "lerna run build --stream --parallel",
    "test": "lerna run test --parallel",
    "client:start": "lerna run start --scope client --stream",
    "client:build": "lerna run build --scope client --stream",
    "client:watch": "lerna run watch --scope client --stream",
    "database:migrate:dev": "npx prisma migrate dev",
    "database:migrate:deploy": "npx prisma migrate deploy",
    "prettier": "prettier src/**/*.{ts,tsx} --write",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "gcr:deploy": "dotenv -e .env node deploy.js",
    "gcr:start": "yarn build && yarn start"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "v1",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.5.2",
    "@supabase/supabase-js": "^2.38.4",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dotenv-cli": "^7.3.0",
    "fs": "^0.0.1-security",
    "heroicons": "^2.0.18",
    "jest": "^29.7.0",
    "lerna": "^7.4.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-query-auth": "^1.0.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "socket.io": "^4.7.2",
    "tailwind": "^3.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "ws": "^8.14.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "webpack-cli": "^5.1.4"
  }
}
